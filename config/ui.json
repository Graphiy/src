{
  "name" : "desktop",
  "resources_path" : "/app",
  "js_path" : "/app",
  "widget" : {
    "name": "widget",
    "element": "widget",
    "position": "top"
  },
  "themes" : {
    "mybase" : {
      "name" : "MyBase",
      "path" : "/app/theme/mybase",
      "element" : "desktop",
      "template" : "/app/theme/mybase/template.json"
    },
    "manager" : {
      "name" : "Manager",
      "path" : "/app/theme/mybase",
      "element" : "desktop",
      "template" : "/app/theme/manager/template.json"
    }
  },
  "hooks": {
    "serverCallback": {
      "hookName": "node.info",
      "methodName": "nodeBookmarkMetadata"
    }
  },
  "component_config": {
    "className": "List",
    "property": [
      {
        "name": "thumbSize",
        "value": "64"
      },
      {
        "name": "displayMode",
        "value": "list"
      }
    ]
