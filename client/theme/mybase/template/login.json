{
  "id": "loginForm",
  "name": "loginForm",
  "control": "CLogin",
  "options": {
    "title": "Enter login/password",
    "labelWidth": 80,
    "header": false, 
    "bodyPadding": 5,
    "height": 140,
    "width": 300,
    "layout": "anchor",
    "defaultType": "textfield",
    "monitorValid": true,
    "items": [{ 
        "itemId": "username",
        "name": "username", 
        "allowBlank": false 
      },{ 
        "itemId": "password",
        "name": "password", 
        "inputType": "password", 
        "allowBlank": false 
      }
    ],
    "buttons": [{
        "itemId": "reset"
      }, {
        "itemId": "submit",
        "formBind": true,
        "disabled": true
      }
    ]
  }
}
