{
  "name": "loginForm",
  "labelWidth": 80,
  "header": false, 
  "bodyPadding": 5,
  "height": 100,
  "width": 300,
  "layout": "anchor",
  "defaultType": "textfield",
  "monitorValid": true,
  "items": [{ 
      "itemId": "username",
      "fieldLabel": "Username", 
      "name": "username", 
      "allowBlank": false 
    },{ 
      "itemId": "password",
      "fieldLabel": "Password", 
      "name": "password", 
      "inputType": "password", 
      "allowBlank": false 
    }
  ],
  "buttons": [{
      "itemId": "reset",
      "text": "Reset"
    }, {
      "itemId": "submit",
      "text": "Submit",
      "formBind": true,
      "disabled": true
    }
  ]
}
