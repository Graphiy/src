{
  "editor": {
    "id": "editor.ckeditor",
    "enabled": "false",
    "openable": "true",
    "order": "-1",
    "text": "319",
    "title": "320",
    "icon": "ckeditor_image/html_edit.png",
    "className": "CkEditor",
    "mimes": "html,htm",
    "formId": "edit_box",
    "write": "true",
    "label": "CONF_MESSAGE[CKEditor]",
    "description": "CONF_MESSAGE[Wysiwyg HTML Editor, requires separate library to be downloaded.]",
    "xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance",
    "xsi:noNamespaceSchemaLocation": "file:../core.app/registry.xsd",
    "dependencies": {
      "pluginResources": {
        "pluginName": "editor.text"
      },
      "activePlugin": {
        "pluginName": "access.AJXP_STREAM_PROVIDER"
      }
    },
    "client_settings": {
      "resources": {
        "js": [
          {
            "file": "plugins/editor.ckeditor/ckeditor/ckeditor.js",
            "className": "CKEDITOR"
          },
          {
            "file": "plugins/editor.ckeditor/class.CkEditor.js",
            "className": "CkEditor"
          }
        ],
        "img_library": {
          "alias": "ckeditor_images",
          "path": "plugins/editor.ckeditor"
        },
        "i18n": {
          "namespace": "ckeditor",
          "path": "plugins/editor.ckeditor/resources/i18n"
        }
      }
    },
    "clientForm": {
      "id": "ck_box",
      "$t": "<script type=\"text/javascript\">window.CKEDITOR_BASEPATH = 'plugins/editor.ckeditor/ckeditor/';</script>"
    }
  }
}
