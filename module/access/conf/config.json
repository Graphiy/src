{
  "ajxpdriver": {
    "name": "conf",
    "label": "CONF_MESSAGE[Admin Driver]",
    "description": "CONF_MESSAGE[Access AjaXplorer configuration data.]",
    "xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance",
    "xsi:noNamespaceSchemaLocation": "file:../core.app/registry.xsd",
    "client_settings": {
      "icon": "plugins/access.conf/admin.png",
      "resources": {
        "i18n": [
          {
            "namespace": "conf",
            "path": "plugins/access.conf/i18n"
          },
          {
            "namespace": "shared",
            "path": "plugins/access.shared/i18n"
          }
        ],
        "js": {
          "file": "plugins/access.conf/class.ConfigEditor.js",
          "className": "ConfigEditor"
        }
      }
    },
    "registry_contributions": {
      "client_configs": {
        "template_part": {
          "theme": "mybase",
          "id": "sidebar_splitter",
          "ajxpClass": "Pane",
          "options": "{\"fit\":\"height\"}",
          "$t": "<div id=\"folderPane\" ajxpClass=\"FoldersTree\" options='{\"display\":\"dz\",\"replaceScroller\":true}' paneHeader=\"165\" paneIcon=\"view_tree.png\" style=\"margin:20px;\"></div><style>#sidebar_splitter{padding-bottom:20px;background-color:#EFD585;}.webfx-tree-item:first-of-type{display:none;}.webfx-tree-container>.webfx-tree-item:first-of-type{display:block;}.webfx-tree-container.first_container{margin-left: -19px;}.webfx-tree-container > .webfx-tree-item {font-weight: bold;}.webfx-tree-container > .webfx-tree-container > .webfx-tree-item {font-weight: normal;}</style>"
        }
      },
      "external_file": [
        {
          "filename": "plugins/access.conf/confActions.xml",
          "include": "action/*"
        },
        {
          "filename": "plugins/access.ajxp_shared/manifest.xml",
          "include": "registry_contributions/action/action[@name=\"clear_expired\"]"
        },
        {
          "filename": "plugins/access.conf/confActions.xml",
          "include": "client_configs/*"
        }
      ]
    },
    "class_definition": {
      "filename": "plugins/access.conf/class.confAccessDriver.php",
      "classname": "confAccessDriver"
    }
  },
	"client_configs" : {
		"component_config" : {
			"className" : "InfoPanel",
			"infoPanel" : [{
					"mime" : "generic_file",
					"attributes" : "icon,text",
					"messages" : {
						"message" : [{
								"key" : "name_string",
								"id" : "133"
							}, {
								"key" : "size_string",
								"id" : "127"
							}, {
								"key" : "type_string",
								"id" : "134"
							}, {
								"key" : "modif_string",
								"id" : "138"
							}
						]
					},
					"html" : "<div style=\"padding:10px;\"><div class=\"folderImage\"><img src=\"AJXP_THEME_FOLDER/image/mimes/64/#{icon}\" height=\"64\" width=\"64\"></div><b>#{name_string}</b> : #{text}</div>"
				}, {
					"mime" : "generic_dir",
					"attributes" : "icon,text",
					"messages" : {
						"message" : [{
								"key" : "name_string",
								"id" : "133"
							}, {
								"key" : "modif_string",
								"id" : "138"
							}
						]
					},
					"html" : "<div style=\"padding:10px;\"><div class=\"folderImage\"><img src=\"AJXP_THEME_FOLDER/image/mimes/64/#{icon}\" height=\"64\" width=\"64\"></div><b>#{name_string}</b> : #{text}</div>"
				}, {
					"mime" : "no_selection",
					"attributes" : "",
					"messages" : {
						"message" : {
							"key" : "files_string",
							"id" : "265"
						}
					},
					"html" : "<div style=\"padding:10px;\"><div class=\"folderImage\"><img src=\"AJXP_THEME_FOLDER/image/mimes/64/admin.png\" height=\"64\" width=\"64\"></div></div>"
				}
			]
		}
	}
}
